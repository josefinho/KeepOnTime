<% tarefas.forEach((tarefa)=> { %>
    <tr>
        <td>
            <div class="mb-3 form-check">
                <input type="checkbox" class="form-check-input" id="exampleCheck1">
                <label class="form-check-label" for="exampleCheck1"></label>
            </div>
        </td>
        <td>TR</td>
        <td>
            <%= tarefa.titulo %>
        </td>
        <td>
            <%= tarefa.prazo %>
        </td>
        <td>
            <%= tarefa.hora %>
        </td>
        <td>
            <%= tarefa.descricao %>
        </td>
    </tr>
    <% }) %>