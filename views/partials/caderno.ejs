<% if(cadernos.length === 0) { %>
    <li class="list-group-item">
        <h4 class="text-center">Sem cadernos</h4>
    </li>
<% } else { %>
    <% cadernos.forEach((caderno)=> { %>
        <a href="#" id="<%= caderno.id %>" tabindex="0" class="list-group-item list-group-item-action">
            <%= caderno.nome %>
        </a>
    <% }) %>
<% } %>